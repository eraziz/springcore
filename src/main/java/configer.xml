<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans   
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
     http://www.springframework.org/schema/context 
     http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean class="com.springcore.Student" name="student1">
		<property name="stdid">
			<value>101</value>
		</property>
		<property name="stdname">
			<value>azizkhan</value>
		</property>
		<property name="stdAddress">
			<value>Mumbai</value>
		</property>
	</bean>
<bean class="com.springcore.Student" name="student2">
		<property name="stdid" value="103"/>
			<property name="stdname" value="Tanzeel"/>
		  <property name="stdAddress" value="pune"/>
		</bean>
		
		<bean class="com.springcore.Student" name="student3" p:stdid="104" p:stdname="kashif" p:stdAddress="mumbai"/>
		
</beans> 